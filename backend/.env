ferr# Local Postgres for development
DATABASE_URL=postgresql://kycc_user:kycc_pass@localhost:5433/kycc_db
# Auto-create tables when using SQLite or dev container. Keep as 0 for production.
AUTO_CREATE_TABLES=1
# Optional: set DEV_DATABASE_URL for sqlite fallback (not used when Docker/Postgres is running)
DEV_DATABASE_URL=sqlite:///./dev.db
